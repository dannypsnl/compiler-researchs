\import Arith.Int
\open IntRing

\record vec (x y z : Int)

\func vec-dot (v1 v2 : vec) =>
  v1.x * v2.x + v1.y * v2.y + v1.z * v2.z
\func vec-length (v : vec) =>
  -- TODO: sqrt this
  (vec-dot v v)

\func test1 : vec-dot (\new vec 0 0 1) (\new vec 0 0 1) = 1 => idp

